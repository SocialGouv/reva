{
  "name": "reva-frontend",
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "elm:watch": "chokidar 'elm_app/src/**/!(Main).elm' -c 'touch elm_app/src/Main.elm'",
    "dev": "concurrently -n \"elm,nuxt\" -c \"blue,green\" -k \"yarn elm:watch\" \"yarn nuxt\"",
    "dev_old": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start --port $PORT",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.ts,.vue\" .",
    "prepare": "husky install",
    "lint": "yarn lint:js",
    "tu": "jest",
    "test": "cypress run",
    "test:ci": "yarn start & wait-on --timeout 6000 http://localhost:3000 && npm run test",
    "db-migrate": "db-migrate",
    "postinstall": "cp -r node_modules/tarteaucitronjs static/. && [[ ! -z \"$DATABASE_URL\" ]] && yarn db-migrate up -e prod || echo \"No migration\""
  },
  "lint-staged": {
    "*.{js,vue}": "eslint"
  },
  "dependencies": {
    "@nuxt/content": "1.14.0",
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.28.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/sentry": "^5.1.4",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/line-clamp": "^0.2.1",
    "@tailwindcss/typography": "^0.4.1",
    "@xstate/inspect": "^0.5.0",
    "argon2": "^0.28.2",
    "body-parser": "^1.19.0",
    "canvas-confetti": "^1.4.0",
    "core-js": "^3.15.1",
    "csv-parser": "^3.0.0",
    "db-migrate": "^0.11.12",
    "db-migrate-pg": "^1.2.2",
    "elm": "^0.19.1-5",
    "elm-format": "^0.8.5",
    "elm-webpack-loader": "^8.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "node-sass": "^6.0.1",
    "nuxt": "^2.15.7",
    "nuxt-matomo": "^1.2.4",
    "pg": "^8.7.1",
    "sass": "^1.41.0",
    "sass-loader": "10.1.1",
    "tarteaucitronjs": "^1.9.4",
    "wait-on": "^6.0.0",
    "xstate": "^4.23.4",
    "xstate-vue2": "^0.2.1",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@nuxt/types": "^2.15.7",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxtjs/eslint-config-typescript": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/tailwindcss": "^4.2.0",
    "@types/jest": "^27.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^6.3.0",
    "cypress": "^8.7.0",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "husky": "^6.0.0",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.4",
    "postcss": "^8.3.6",
    "prettier": "^2.5.1",
    "ts-jest": "^27.0.7",
    "vue-jest": "^3.0.4"
  }
}
