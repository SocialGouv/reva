{
  "name": "reva-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.7.9",
    "@capacitor/android": "^4.0.0",
    "@capacitor/device": "^4.0.0",
    "@capacitor/ios": "^4.0.0",
    "@capacitor/splash-screen": "^4.0.1",
    "@capacitor/status-bar": "^4.0.0",
    "@codegouvfr/react-dsfr": "^0.33.2",
    "@headlessui/react": "^1.6.6",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@xstate/react": "^3.2.1",
    "classnames": "^2.3.1",
    "crisp-sdk-web": "^1.0.4",
    "html-react-parser": "^1.4.8",
    "keycloak-js": "^19.0.3",
    "purify-ts": "^1.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "web-vitals": "^0.2.4",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4",
    "xstate": "^4.30.6"
  },
  "scripts": {
    "dev": "react-scripts start",
    "dev:mobile": "CAPACITOR_LIVE_RELOAD=1 npx cap sync && PUBLIC_URL=/ react-scripts start",
    "build": "react-scripts build && cp -R ./build ../reva-api/dist/app",
    "build:mobile": "REACT_APP_API_GRAPHQL=https://reva.beta.gouv.fr/graphql PUBLIC_URL=/ npm run build && npx cap sync",
    "build:resources": "cordova-res ios --skip-config --copy && cordova-res android --skip-config --copy && npx cap sync",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "postinstall": "cp -Rf ../../node_modules/@codegouvfr/react-dsfr/dsfr ./public",
    "predev": "only-include-used-icons",
    "prebuild": "only-include-used-icons"
  },
  "homepage": "/app",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "react-hooks"
    ],
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "warn"
    }
  },
  "importSort": {
    ".js, .ts, .jsx, .tsx": {
      "style": "module",
      "parser": "typescript"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@capacitor/cli": "^4.0.0",
    "@capacitor/core": "^4.0.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.46",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-helmet": "^6.1.6",
    "@xstate/inspect": "^0.7.0",
    "autoprefixer": "^10.4.2",
    "esbuild": "^0.15.5",
    "eslint-plugin-react-hooks": "^4.3.0",
    "framer-motion": "^6.2.8",
    "import-sort-style-module": "^6.0.0",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1"
  }
}
