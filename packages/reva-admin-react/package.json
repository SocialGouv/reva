{
  "name": "reva-admin-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "only-include-used-icons && graphql-codegen --config codegen.ts",
    "dev": "next dev -p 3003",
    "prebuild": "only-include-used-icons && npm i --include=dev --prefix ../reva-api && npm run graphql:generate --prefix ../reva-api && graphql-codegen --config codegen.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "graphql:generate": "graphql-codegen --config codegen.ts",
    "graphql:watch": "graphql-codegen --config codegen.ts --watch",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "test:ci": "start-server-and-test 'next start -p 3003' http://localhost:3003/admin2/ cypress:ci"
  },
  "dependencies": {
    "@codegouvfr/react-dsfr": "^1.14.1",
    "@date-fns/tz": "^1.2.0",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@hookform/resolvers": "^3.9.1",
    "@strapi/blocks-react-renderer": "^1.0.1",
    "@tanstack/react-query": "^5.59.16",
    "@types/crypto-js": "^4.2.2",
    "@urql/exchange-auth": "^2.2.0",
    "crisp-sdk-web": "^1.0.25",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "graphql": "^16.9.0",
    "graphql-request": "^6.1.0",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "jwt-decode": "^4.0.0",
    "keycloak-js": "^24.0.3",
    "lodash": "^4.17.21",
    "next": "14.2.21",
    "postcss-import": "^16.1.0",
    "postcss-url": "^10.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.53.1",
    "react-hot-toast": "^2.4.1",
    "sharp": "^0.33.5",
    "urql": "^4.2.0",
    "uuid": "^11.0.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/client-preset": "4.5.0",
    "@parcel/watcher": "^2.5.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "cypress": "^13.15.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.16",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "start-server-and-test": "^2.0.8",
    "tailwindcss": "^3.4.14",
    "typescript": "5.4.5"
  }
}
